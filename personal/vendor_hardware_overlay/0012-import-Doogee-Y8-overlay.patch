From: Victor Bo <bvoid@yandex.ru>
Date: Thu, 12 Aug 2021 09:32:36 +0000
Subject: [PATCH 12/21] import Doogee Y8 overlay

  from https://github.com/sooti/vendor_hardware_overlay
---
 Doogee/Y8/Android.mk            |  8 ++++++++
 Doogee/Y8/AndroidManifest.xml   | 10 ++++++++++
 Doogee/Y8/res/values/config.xml | 25 +++++++++++++++++++++++++
 Doogee/Y8/res/values/notch.xml  | 24 ++++++++++++++++++++++++
 overlay.mk                      |  1 +
 5 files changed, 68 insertions(+)
 create mode 100644 Doogee/Y8/Android.mk
 create mode 100644 Doogee/Y8/AndroidManifest.xml
 create mode 100644 Doogee/Y8/res/values/config.xml
 create mode 100644 Doogee/Y8/res/values/notch.xml

diff --git a/Doogee/Y8/Android.mk b/Doogee/Y8/Android.mk
new file mode 100644
index 0000000..6ee9a26
--- /dev/null
+++ b/Doogee/Y8/Android.mk
@@ -0,0 +1,8 @@
+LOCAL_PATH := $(call my-dir)
+include $(CLEAR_VARS)
+LOCAL_MODULE_TAGS := optional
+LOCAL_PACKAGE_NAME := treble-overlay-doogee-y8
+LOCAL_MODULE_PATH := $(TARGET_OUT)/product/overlay
+LOCAL_IS_RUNTIME_RESOURCE_OVERLAY := true
+LOCAL_PRIVATE_PLATFORM_APIS := true
+include $(BUILD_PACKAGE)
diff --git a/Doogee/Y8/AndroidManifest.xml b/Doogee/Y8/AndroidManifest.xml
new file mode 100644
index 0000000..d36ce33
--- /dev/null
+++ b/Doogee/Y8/AndroidManifest.xml
@@ -0,0 +1,10 @@
+<manifest xmlns:android="http://schemas.android.com/apk/res/android"
+        package="me.phh.treble.overlay.umidigi.y8"
+        android:versionCode="1"
+        android:versionName="1.0">
+        <overlay android:targetPackage="android"
+                android:requiredSystemPropertyName="ro.product.vendor.model"
+		android:requiredSystemPropertyValue="Y8"
+		android:priority="114"
+		android:isStatic="true" />
+</manifest>
