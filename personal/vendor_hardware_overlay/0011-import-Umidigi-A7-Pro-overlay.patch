Subject: [PATCH 11/21] import Umidigi A7 Pro overlay

  from https://github.com/sooti/vendor_hardware_overlay
---
 Umidigi/A7Pro/Android.mk            |  8 +++++
 Umidigi/A7Pro/AndroidManifest.xml   | 10 ++++++
 Umidigi/A7Pro/res/values/colors.xml | 26 ++++++++++++++++
 Umidigi/A7Pro/res/values/config.xml | 47 +++++++++++++++++++++++++++++
 Umidigi/A7Pro/res/values/notch.xml  | 25 +++++++++++++++
 overlay.mk                          |  1 +
 6 files changed, 117 insertions(+)
 create mode 100644 Umidigi/A7Pro/Android.mk
 create mode 100644 Umidigi/A7Pro/AndroidManifest.xml
 create mode 100644 Umidigi/A7Pro/res/values/colors.xml
 create mode 100644 Umidigi/A7Pro/res/values/config.xml
 create mode 100644 Umidigi/A7Pro/res/values/notch.xml

diff --git a/Umidigi/A7Pro/Android.mk b/Umidigi/A7Pro/Android.mk
new file mode 100644
index 0000000..fa9e7e3
--- /dev/null
+++ b/Umidigi/A7Pro/Android.mk
@@ -0,0 +1,8 @@
+LOCAL_PATH := $(call my-dir)
+include $(CLEAR_VARS)
+LOCAL_MODULE_TAGS := optional
+LOCAL_PACKAGE_NAME := treble-overlay-umidigi-a7pro
+LOCAL_MODULE_PATH := $(TARGET_OUT)/product/overlay
+LOCAL_IS_RUNTIME_RESOURCE_OVERLAY := true
+LOCAL_PRIVATE_PLATFORM_APIS := true
+include $(BUILD_PACKAGE)
diff --git a/Umidigi/A7Pro/AndroidManifest.xml b/Umidigi/A7Pro/AndroidManifest.xml
new file mode 100644
index 0000000..0c074d3
--- /dev/null
+++ b/Umidigi/A7Pro/AndroidManifest.xml
@@ -0,0 +1,10 @@
+<manifest xmlns:android="http://schemas.android.com/apk/res/android"
+        package="me.phh.treble.overlay.umidigi.a7pro"
+        android:versionCode="1"
+        android:versionName="1.0">
+        <overlay android:targetPackage="android"
+                android:requiredSystemPropertyName="ro.product.vendor.model"
+		android:requiredSystemPropertyValue="A7 Pro"
+		android:priority="114"
+		android:isStatic="true" />
+</manifest>
