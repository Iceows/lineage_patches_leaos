diff --git a/Realme/RMX2061/Android.mk b/Realme/RMX2061/Android.mk
new file mode 100644
index 0000000..af92104
--- /dev/null
+++ b/Realme/RMX2061/Android.mk
@@ -0,0 +1,8 @@
+LOCAL_PATH := $(call my-dir)
+include $(CLEAR_VARS)
+LOCAL_MODULE_TAGS := optional
